{
    "env": {
        "browser": true,
        "node": false
    },
    "parser": "babel-eslint",
    "extends": ["eslint:recommended", "standard"],
    "rules": {
        "no-console": 0,
        "no-useless-call": 0,
        "no-unused-vars": ["error", { "args": "after-used" }],
        "indent": ["error", 4, { "SwitchCase": 1 }],
        "semi": ["error", "always"],
        "padded-blocks": 0
    }
}